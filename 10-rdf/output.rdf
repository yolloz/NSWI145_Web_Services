
@prefix vso: <http://purl.org/vso/ns#> .
@prefix mnfc: <http://example.org/manufacturer/> .
@prefix dbpedia-owl: <http://dbpedia.org/ontology/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
    
    <http://example.org/manufacturer/manufacturers/Ford> a mnfc:Manufacturer ;
        dbpedia-owl:name "Ford"^^xsd:string .

    
    <http://example.org/manufacturer/models/T> a mnfc:VehicleModel ;
        dbpedia-owl:name "T"^^xsd:string .

    
    <http://example.org/manufacturer/plants/Highland%20Park%2C%20Michigan> a mnfc:ManufacturingPlant ;
        dbpedia-owl:name "Highland Park, Michigan"^^xsd:string .

    
    <http://example.org/manufacturer/AAA12345678912345> a vso:MotorizedRoadVehicle ;
        mnfc:manufacturedBy <http://example.org/manufacturer/manufacturers/Ford> ;
        mnfc:modelOf <http://example.org/manufacturer/models/T> ;
        mnfc:manufacturedIn <http://example.org/manufacturer/plants/Highland%20Park%2C%20Michigan> .
